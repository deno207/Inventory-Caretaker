<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<StackPane xmlns="http://javafx.com/javafx"
           xmlns:fx="http://javafx.com/fxml"
           maxHeight="200.0" maxWidth="200.0" fx:id="stackPane" stylesheets="/css/main.css" styleClass="grid-item">
    <ImageView fx:id="itemImage" styleClass="grid-item" fitWidth="200" fitHeight="200"/>
    <Label fx:id="itemName" styleClass="grid-item" maxWidth="200" prefWidth="200" StackPane.alignment="BOTTOM_CENTER"
            textAlignment="CENTER"/>
</StackPane>
